DATABASE_URL=postgresql://user:password@localhost/dashboard
REDIS_URL=redis://localhost:6379/0

# App
SECRET_KEY=your-secret-key-here-change-in-production
ENCRYPTION_KEY=  # 32-byte base64 encoded key for token encryption
DEBUG=false

# API Token Protection (设置后启用访问控制)
DASHBOARD_API_TOKEN=your-secure-api-token-here

# GitHub OAuth App
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret
GITHUB_REDIRECT_URI=http://localhost:5173/auth/github/callback

# GitHub Personal Access Token (fallback if OAuth not used)
GITHUB_TOKEN=your_github_personal_access_token
GITHUB_USERNAME=your_github_username

# Trello
TRELLO_API_KEY=your_trello_key
TRELLO_TOKEN=your_trello_token
TRELLO_BOARD_ID=optional_board_id

# Weather (Open-Meteo 免费，不需要 API Key)
OPENWEATHER_API_KEY=optional